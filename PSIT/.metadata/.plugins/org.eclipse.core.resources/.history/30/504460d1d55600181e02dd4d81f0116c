package Practice;

public class FirstTenPrime {

	/**
	 * @param args
	 */
	public static void main(String[] args) 
	{
	       int number=100;
	      int count=0;
	       int i,j;
	       for(i=2;i<=count+1;i++)
	       {
	    	   for(j=2;j<=i;j++)
	    	   {
	    		   if(j==i)
	    		   {
	    			   System.out.println(i);
	    			   count=count+1;
	    		   }
	    		   if(i%j==0)
	    		   {
	    			   break;
	    		   }
	    	   }
	    	   if(count==5)
	    	   {
	    		   break;
	    	   }
	       }
	}

}
